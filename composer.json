{
    "name": "albinvar/termux-webzone",
    "description": "The Termux Webzone CLI App.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/albinvar/termux-webzone",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/albinvar/termux-webzone/issues",
        "source": "https://github.com/albinvar/termux-webzone"
    },
    "authors": [
        {
            "name": "Albin Varghese",
            "email": "albin8285@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-zip": "*",
        "guzzlehttp/guzzle": "^6.5.5",
        "illuminate/http": "^8.0",
        "illuminate/validation": "^8.35",
        "laminas/laminas-text": "^2.8",
        "padraic/phar-updater": "^1.0.6"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.4",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/phpinsights": "^2.0",
        "pestphp/pest": "^1.0",
        "phpstan/phpstan": "^0.12.89"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/webzone"]
}
